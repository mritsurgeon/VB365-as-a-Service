{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "connections_conversionservice_name": {
            "defaultValue": "conversionservice",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Web/connections",
            "apiVersion": "2016-06-01",
            "name": "[parameters('connections_conversionservice_name')]",
            "location": "eastus",
            "kind": "V2",
            "properties": {
                "displayName": "Content Conversion",
                "customParameterValues": {},
                "nonSecretParameterValues": {},
                "api": {
                    "name": "[parameters('connections_conversionservice_name')]",
                    "displayName": "Content Conversion",
                    "description": "A service that allows content to be converted from one format to another.",
                    "iconUri": "[concat('https://connectoricons-prod.azureedge.net/u/laborbol/releases/ase-v3/1.0.1622.3202/', parameters('connections_conversionservice_name'), '/icon.png')]",
                    "brandColor": "#4f6bed",
                    "type": "Microsoft.Web/locations/managedApis"
                },
                "testLinks": []
            }
        }
    ]
}
